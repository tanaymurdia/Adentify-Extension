{
  "manifest_version": 3,
  "name": "Adentify",
  "version": "0.1.0",
  "description": "Adentify screen capture with overlay.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabCapture",
    "tabs",
    "windows"
  ],
  "optional_permissions": [
    "desktopCapture",
    "offscreen"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_icon": {
      "128": "icon128.png"
    },
    "default_title": "Start Recording",
    "default_popup": "launcher.html"
  },
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [],
  "web_accessible_resources": [
    {
      "resources": [
        "offscreen.html",
        "offscreen.bundle.js",
        "launcher.html",
        "launcher.bundle.js",
        "launcher.css",
        "icon128.png",
        "onnx_worker.bundle.js",
        "cast/cast_helpers.js",
        "cast/cast_framework.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  }
} 